{
  "manifest_version": 3,
  "name": "搬瓦工服务器流量监控",
  "version": "1.1",
  "description": "多服务器流量实时监控",
  "permissions": [
      "storage",
      "activeTab"
  ],
  "host_permissions": [
      "*://api.64clouds.com/*"
  ],
  "action": {
      "default_popup": "popup.html"
  },
  "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self';"
  }
}